// prisma/schema.prisma
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model PsaScan {
  id              Int      @id @default(autoincrement())
  url             String
  urlHash         String   @unique
  pageTitle       String?
  privacyScore    String
  privacyNote     String
  securityScore   String
  securityNote    String
  ageScore        String
  ageNote         String
  hasSchema       String?
  hasCookieBanner String?
  createdAt       DateTime @default(now())
  updatedAt       DateTime @updatedAt
}
